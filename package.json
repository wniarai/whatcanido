{
  "name": "whatcanido",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register test/**/*.ts",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js --port 3588",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js --ignore-watch='source/*' --port 3588"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/busboy": "^2.1.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/mysql": "^4.3.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-json-schema-to-ts": "^3.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/pump": "^1.1.3",
    "@types/uuid": "^10.0.0",
    "cheerio": "1.0.0-rc.12",
    "close-with-grace": "^1.3.0",
    "concurrently": "^8.2.2",
    "docxtemplater": "^3.48.0",
    "fastify": "^4.27.0",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.0.0",
    "fs": "0.0.1-security",
    "glob": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "pizzip": "^3.1.7",
    "pump": "^3.0.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.4.4",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "module-alias": "^2.2.3",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}